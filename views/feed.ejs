<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Vault</title>
    <link rel="stylesheet" href="stylesheets/styleFeed.css">
    <script src="https://kit.fontawesome.com/236ad7d057.js" crossorigin="anonymous"></script>
</head>
<body>
    <nav>
        <div class="nav-left">
            <img src="images/v.png" class="logo">
            <ul>
                <li><img src="images/homeIcon.png"></li>
                <li><img src="images/buildIcon.png"></li>
                <li><img src="images/notificationIcon.png"></li>
            </ul>
        </div>
        <div class="nav-right">
            <div class="search-box">
                <img src="images/searchIcon.png">
                <input type="text" placeholder="search">
            </div>
            <div class="nav-user-icon online">
                <img src="images/avatarIcon.png">
            </div>
            <div class="nav-user-icon">
                <img src="images/settingsIcon.png">
            </div>
        </div>
    </nav>

    <div class="container">
        <!-- Left Sidebar -->
        <div class="left-sidebar">
            <div class="imp-links">
                <a href="#"><img src="images/settingsIcon.png">Settings</a>
                <a href="#"><img src="images/settingsIcon.png">Market Place</a>
                <a href="#"><img src="images/settingsIcon.png">Sell Your Home</a>
                <a href="#"><img src="images/settingsIcon.png">Watch</a>
                <a href="#">See More</a>
            </div>
            <div class="shortcut-links">
                <p>Your Shortcuts</p>
                <a href="#"><img src="images/settingsIcon.png">Home Renovation</a>
                <a href="#"><img src="images/settingsIcon.png">Extensions</a>
                <a href="#"><img src="images/settingsIcon.png">Driveways</a>
                <a href="#"><img src="images/settingsIcon.png">Luxury Homes</a>
            </div>
        </div>
        <!-- Main Content -->
        <div class="main-content">
            
        </div>
        <!-- Right Sidebar -->
        <div class="right-sidebar">
            <div class="sidebar-title">
                <h4>Events</h4>
                <a href="#">See All</a>
            </div>
            <div class="event">
                <div class="left-event">
                    <h3>18th</h3>    
                    <span>March</span>
                </div>
                <div class="right-event">
                    <h4>Social Media</h4>
                    <p><i class="fa-sharp fa-solid fa-location-dot" style="color: #929292;"></i> Ipswich</p>
                    <a href="#">More</a>
                </div>
            </div>
            <div class="event">
                <div class="left-event">
                    <h3>8th</h3>    
                    <span>February</span>
                </div>
                <div class="right-event">
                    <h4>Cole's Birthday</h4>
                    <p><i class="fa-sharp fa-solid fa-location-dot" style="color: #929292;"></i> Ipswich</p>
                    <a href="#">More</a>
                </div>
            </div>
            <div class="sidebar-title">
                <h4>Advertisements</h4>
                <a href="#">Close</a>
            </div>
            <img src="images/advertisement.png" class="sidebar-ads">
            <div class="sidebar-title">
                <h4>Friends</h4>
                <a href="#">Hide</a>
            </div>
            <div class="online-list">
                <div class="online">
                    <img src="images/avatarIcon.png">
                </div>
                <p>Lara Vockins</p>
            </div>
            <div class="online-list">
                <div class="online">
                    <img src="images/avatarIcon.png">
                </div>
                <p>Alahna Brierly</p>
            </div>
        </div>
    </div>

</body>
</html>